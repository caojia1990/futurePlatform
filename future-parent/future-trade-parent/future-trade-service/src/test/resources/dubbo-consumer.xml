<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	   http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd
	   ">
	<dubbo:application name="future-trade-consumer" />
	<!-- <dubbo:registry address="N/A" /> -->
	
	<dubbo:registry protocol="zookeeper" address="${dubbo.registry.address}" check="false" />

	<dubbo:reference id="tradeService" interface="com.future.trade.api.service.TradeService"  timeout="5000" check="false"/>
	
</beans>
